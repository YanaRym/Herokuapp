<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Test" thread-count="3" parallel="classes">
    <listeners>
        <listener class-name="utilities.AnnotationTransformer"/>
    </listeners>
    <parameter name="browser" value="firefox"/>
    <test name="Herokuapp Test">
        <classes>
            <class name="tests.CheckboxesClassicalPomTest"/>
            <class name="tests.InputsClassicalPomTest"/>
            <class name="tests.NotificationMessageFactoryTest"/>
            <class name="tests.SortableDataTablesBusinessPomTest"/>
            <class name="tests.TyposClassicalPomTest"/>
            <class name="tests.FileUploadTest"/>
            <class name="tests.DropdownListTest"/>
            <class name="tests.JSAlertTest"/>
            <class name="tests.JSConfirmTest"/>
            <class name="tests.JSPromptTest"/>
        </classes>
    </test>
    <test name="Jqueryui Test">
        <classes>
            <class name="tests.DroppableTest"/>
        </classes>
    </test>
</suite>
